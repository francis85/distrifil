<h1>Recibo list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Forma_pago</th>
            <th>Total</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('distrifil_recibo_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.formapago }}</td>
            <td>{{ entity.total }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('distrifil_recibo_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('distrifil_recibo_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('distrifil_recibo_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
